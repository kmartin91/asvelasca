{
  "name": "velasca",
  "version": "1.0.0",
  "description": "Official Website of AS Velasca",
  "main": "index.js",
  "scripts": {
    "test": "cross-env BABEL_ENV=test jest --maxWorkers=4 --config ./tools/config/test/jest.cover.json",
    "jest": "cross-env BABEL_ENV=test jest --maxWorkers=4 --config ./tools/config/test/jest.cover.json",
    "start": "parcel index.html",
    "dev": "parcel index.html",
    "build": "parcel build index.html && cp _redirects dist/_redirects",
    "prettier:js": "prettier --write \"src/**/*.js\"",
    "prettier:css": "prettier --write \"src/**/*.scss\"",
    "jeveuxdujoliJS": "prettier --write \"src/**/*.js\"",
    "makeCSSGreatAgain": "prettier --write \"src/**/*.scss\"",
    "lint": "lint --ignore-pattern"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmartin91/asvelasca.git"
  },
  "author": "Kevin Martin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kmartin91/asvelasca/issues"
  },
  "homepage": "https://github.com/kmartin91/asvelasca#readme",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "flow": "^0.2.3",
    "jest": "^25.2.1",
    "lodash": "^4.17.15",
    "marked": "^0.8.2",
    "parcel-bundler": "^1.12.4",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-imported-component": "^6.2.1",
    "react-router-dom": "^5.1.2",
    "sass-resources-loader": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "flow-bin": "^0.121.0",
    "flow-typed": "^3.1.0",
    "husky": "^4.2.3",
    "jest-environment-jsdom-global": "^1.2.1",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "sass": "^1.26.3",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-selector-bem-pattern": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn flow"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 1 Chrome version",
    "Chrome > 49",
    "ChromeAndroid > 49",
    "Firefox > 46",
    "FirefoxAndroid > 46",
    "Safari > 10",
    "iOS > 10",
    "Opera > 36",
    "Edge > 15"
  ]
}
